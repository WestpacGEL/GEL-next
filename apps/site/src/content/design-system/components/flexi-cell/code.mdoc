      accounts: [
        {
          name: 'US Dollar Currency Account',
          amount: '$3,957.57',
          number: '123-456 1234567',
        },
        {
          name: 'British pound Currency Account',
          amount: '$9,999,999,999.99',
          number: '123-456 1234567',
        },
      ],
