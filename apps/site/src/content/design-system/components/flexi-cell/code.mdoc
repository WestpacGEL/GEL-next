      payees: [
        {
          initials: 'AJ',
          name: 'Andrew Jones',
          paidAt: 'Fri 5 Aug',
          number: '123-986 463846',
        },
        {
          initials: 'JW',
          name: 'Joss Wight',
          paidAt: 'Mon 1 Aug',
          number: '098-567 465352',
        },
      ],
    },
    {
      title: 'A',
      id: 'a',
      payees: [
        {
          initials: 'AO',
          name: 'Active OOSH',
          number: '857-434 856383',
          paidAt: undefined,
        },
        {
          initials: 'AC',
          name: 'Alice Cartwright',
          number: '950-456 345363',
          paidAt: undefined,
        },
      ],
    },
    {
      title: 'B',
      id: 'b',
      payees: [
        {
          initials: 'BN',
          name: 'Benjamin North',
          number: '098-123 745362',
          paidAt: undefined,
        },
      ],
    },
  ];

  return (
    <>
      {MOCK_PAYEES.map(({ title, id, payees }) => (
        <div key={id}>
          <h3 className="mb-3 border-b-[1px] border-border pb-1 font-normal">{title}</h3>
